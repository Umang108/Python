class Solution(object):
    def minSubarray(self, nums, p):
        """
        :type nums: List[int]
        :type p: int
        :rtype: int
        """
        total = sum(nums)
        r = total % p

        if(r == 0):
            return 0

        prefix = 0
        seen = {0: -1}
        ans = len(nums)

        for i, num in enumerate(nums):
            prefix = (prefix + num) % p
            need = (prefix - r) % p

            if need in seen:
                ans = min(ans, i - seen[need])

            seen[prefix] = i
        return ans if ans < len(nums) else -1

        
